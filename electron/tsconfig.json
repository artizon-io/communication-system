{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "outDir": "after-ts",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"],
  "exclude": [
    "src/main.ts",
    "src/preload.ts",
    "node_modules"
  ]
}
